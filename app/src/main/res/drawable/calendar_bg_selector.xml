<selector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
  <item app:tsquare_state_range_middle="true">
    <color android:color="@color/orange_hi_light"/>
  </item>
  <item android:state_selected="true">
    <color android:color="@color/red_hi"/>
  </item>
  <item android:state_pressed="true">
    <color android:color="@color/red_hi"/>
  </item>
  <item app:tsquare_state_current_month="false">
    <color android:color="@color/bg_hi"/>
  </item>
  <item app:tsquare_state_highlighted="true">
    <color android:color="@color/calendar_highlighted_day_bg"/>
  </item>
  <item app:tsquare_state_today="true">
    <shape android:shape="oval">
      <solid android:color="@color/red_hi"/>
      <stroke
          android:width="4dp"
          android:color="@color/tran_hi"/>
    </shape>
  </item>
  <item>
    <color android:color="@color/white_hi"/>
  </item>
</selector>
